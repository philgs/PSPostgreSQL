        <dev:type>
          <maml:name>System.Management.Automation.PSObject</maml:name>
        </dev:type>
        <maml:description>
          <maml:para>Returns PSObject instances representing each row in the result set. Each object has properties corresponding to the column names in the query result.</maml:para>
        </maml:description>
      </command:returnValue>
    </command:returnValues>
    <maml:alertSet>
      <maml:alert>
        <maml:para>DBNull values are converted to $null in the output objects.</maml:para>
        <maml:para>Use the -Verbose switch to see the number of records returned.</maml:para>
      </maml:alert>
    </maml:alertSet>
    <command:examples>
      <command:example>
        <maml:title>-------------------------- Example 1 --------------------------</maml:title>
        <dev:code>Invoke-PGQuery -Query "SELECT * FROM users"</dev:code>
        <dev:remarks>
          <maml:para>Retrieves all rows from the users table using the session state connection.</maml:para>
        </dev:remarks>
      </command:example>
      <command:example>
        <maml:title>-------------------------- Example 2 --------------------------</maml:title>
        <dev:code>Invoke-PGQuery "SELECT id, name FROM products WHERE price > 100" | Format-Table</dev:code>
