  <command:command xmlns:maml="http://schemas.microsoft.com/maml/2004/10" xmlns:command="http://schemas.microsoft.com/maml/dev/command/2004/10" xmlns:dev="http://schemas.microsoft.com/maml/dev/2004/10" xmlns:MSHelp="http://msdn.microsoft.com/mshelp">
    <command:details>
      <command:name>Invoke-PGNonQuery</command:name>
      <command:verb>Invoke</command:verb>
      <command:noun>PGNonQuery</command:noun>
      <maml:description>
        <maml:para>Executes a SQL command that does not return results, such as INSERT, UPDATE, or DELETE.</maml:para>
      </maml:description>
    </command:details>
    <maml:description>
      <maml:para>The Invoke-PGNonQuery cmdlet executes a SQL command that does not return a result set against a PostgreSQL database. This is typically used for INSERT, UPDATE, DELETE, or DDL statements.</maml:para>
      <maml:para>The cmdlet returns the number of rows affected by the command. If no connection is specified, it uses the connection stored in session state by Connect-PGDatabase.</maml:para>
    </maml:description>
    <command:syntax>
      <command:syntaxItem>
        <maml:name>Invoke-PGNonQuery</maml:name>
        <command:parameter required="true" variableLength="true" globbing="false" pipelineInput="False" position="0" aliases="none">
          <maml:name>Query</maml:name>
          <maml:description>
            <maml:para>The SQL command to execute.</maml:para>
          </maml:description>
          <command:parameterValue required="true" variableLength="false">String</command:parameterValue>
          <dev:type>
            <maml:name>String</maml:name>
            <maml:uri />
          </dev:type>
          <dev:defaultValue>None</dev:defaultValue>
        </command:parameter>
        <command:parameter required="false" variableLength="true" globbing="false" pipelineInput="False" position="named" aliases="none">
          <maml:name>Connection</maml:name>
          <maml:description>
            <maml:para>The NpgsqlConnection object to use. If not specified, uses the connection stored in session state.</maml:para>
          </maml:description>
          <command:parameterValue required="true" variableLength="false">NpgsqlConnection</command:parameterValue>
          <dev:type>
            <maml:name>NpgsqlConnection</maml:name>
            <maml:uri />
          </dev:type>
          <dev:defaultValue>None</dev:defaultValue>
        </command:parameter>
        <command:parameter required="false" variableLength="true" globbing="false" pipelineInput="False" position="named" aliases="none">
          <maml:name>CommandTimeout</maml:name>
          <maml:description>
            <maml:para>The timeout in seconds for the command execution. Overrides the connection's default command timeout.</maml:para>
          </maml:description>
          <command:parameterValue required="true" variableLength="false">Int32</command:parameterValue>
          <dev:type>
            <maml:name>Int32</maml:name>
            <maml:uri />
          </dev:type>
          <dev:defaultValue>30</dev:defaultValue>
        </command:parameter>
      </command:syntaxItem>
    </command:syntax>
    <command:parameters>
      <command:parameter required="true" variableLength="true" globbing="false" pipelineInput="False" position="0" aliases="none">
        <maml:name>Query</maml:name>
        <maml:description>
          <maml:para>The SQL command to execute.</maml:para>
        </maml:description>
