  <!-- Disconnect-PGDatabase -->
  <command:command xmlns:maml="http://schemas.microsoft.com/maml/2004/10" xmlns:command="http://schemas.microsoft.com/maml/dev/command/2004/10" xmlns:dev="http://schemas.microsoft.com/maml/dev/2004/10" xmlns:MSHelp="http://msdn.microsoft.com/mshelp">
    <command:details>
      <command:name>Disconnect-PGDatabase</command:name>
      <command:verb>Disconnect</command:verb>
      <command:noun>PGDatabase</command:noun>
      <maml:description>
        <maml:para>Closes and disposes of a PostgreSQL database connection.</maml:para>
      </maml:description>
    </command:details>
    <maml:description>
      <maml:para>The Disconnect-PGDatabase cmdlet closes an open PostgreSQL database connection and releases all resources associated with it. If no connection is specified, it uses the connection stored in session state by Connect-PGDatabase.</maml:para>
      <maml:para>Always call this cmdlet when you are finished working with the database to ensure proper cleanup of resources.</maml:para>
    </maml:description>
    <command:syntax>
      <command:syntaxItem>
        <maml:name>Disconnect-PGDatabase</maml:name>
        <command:parameter required="false" variableLength="true" globbing="false" pipelineInput="True (ByValue)" position="0" aliases="none">
          <maml:name>Connection</maml:name>
          <maml:description>
            <maml:para>The NpgsqlConnection object to close. If not specified, uses the connection stored in session state.</maml:para>
          </maml:description>
          <command:parameterValue required="true" variableLength="false">NpgsqlConnection</command:parameterValue>
          <dev:type>
            <maml:name>NpgsqlConnection</maml:name>
            <maml:uri />
          </dev:type>
          <dev:defaultValue>None</dev:defaultValue>
        </command:parameter>
      </command:syntaxItem>
    </command:syntax>
    <command:parameters>
      <command:parameter required="false" variableLength="true" globbing="false" pipelineInput="True (ByValue)" position="0" aliases="none">
        <maml:name>Connection</maml:name>
        <maml:description>
          <maml:para>The NpgsqlConnection object to close. If not specified, uses the connection stored in session state.</maml:para>
        </maml:description>
        <command:parameterValue required="true" variableLength="false">NpgsqlConnection</command:parameterValue>
        <dev:type>
          <maml:name>NpgsqlConnection</maml:name>
          <maml:uri />
        </dev:type>
        <dev:defaultValue>None</dev:defaultValue>
      </command:parameter>
    </command:parameters>
    <command:inputTypes>
      <command:inputType>
        <dev:type>
          <maml:name>Npgsql.NpgsqlConnection</maml:name>
        </dev:type>
        <maml:description>
          <maml:para>You can pipe a NpgsqlConnection object to this cmdlet.</maml:para>
        </maml:description>
      </command:inputType>
    </command:inputTypes>
    <command:returnValues>
      <command:returnValue>
        <dev:type>
          <maml:name>None</maml:name>
        </dev:type>
        <maml:description>
          <maml:para>This cmdlet does not generate any output.</maml:para>
        </maml:description>
      </command:returnValue>
    </command:returnValues>
    <maml:alertSet>
      <maml:alert>
        <maml:para>This cmdlet removes the PGSQLConnection variable from session state after closing the connection.</maml:para>
      </maml:alert>
    </maml:alertSet>
    <command:examples>
      <command:example>
        <maml:title>-------------------------- Example 1 --------------------------</maml:title>
        <dev:code>Disconnect-PGDatabase</dev:code>
        <dev:remarks>
          <maml:para>Closes the connection stored in session state.</maml:para>
        </dev:remarks>
      </command:example>
      <command:example>
        <maml:title>-------------------------- Example 2 --------------------------</maml:title>
        <dev:code>$conn = Connect-PGDatabase -Credential $cred
# ... do work ...
$conn | Disconnect-PGDatabase</dev:code>
        <dev:remarks>
          <maml:para>Pipes a connection object directly to Disconnect-PGDatabase to close it.</maml:para>
        </dev:remarks>
      </command:example>
      <command:example>
        <maml:title>-------------------------- Example 3 --------------------------</maml:title>
        <dev:code>Disconnect-PGDatabase -Connection $myConnection</dev:code>
        <dev:remarks>
          <maml:para>Closes a specific connection object passed as a parameter.</maml:para>
        </dev:remarks>
      </command:example>
    </command:examples>
