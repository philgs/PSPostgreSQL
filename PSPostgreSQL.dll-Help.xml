        <dev:code>$cred = Get-Credential
Connect-PGDatabase -ComputerName "dbserver.example.com" -Database "mydb" -Credential $cred</dev:code>
        <dev:remarks>
          <maml:para>Connects to the PostgreSQL database "mydb" on server "dbserver.example.com" using the specified credentials.</maml:para>
